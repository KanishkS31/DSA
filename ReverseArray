#include<iostream>
#include<array>
using namespace std;

int main()
{
    int i,a[]={1,2,3,4,5};
    int n = sizeof(a)/sizeof(a[0]);
    for(i=n-1;i>=0;i--)
    cout<<a[i]<<"";
    cout<<endl;
    
    return 0;
}
